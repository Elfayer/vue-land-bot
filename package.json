{
  "name": "vue-land-bot",
  "version": "0.1.0-beta-1",
  "description": "Official VueLand bot.",
  "main": "src/client.js",
  "scripts": {
    "dev": "npm run build && pm2 start ecosystem.config.js --only vuebot --env development --watch",
    "start": "node -r dotenv/config -r esm dist/vuebot.js",
    "watch": "tsc -p . -w",
    "build": "tsc -p .",
    "build:clean": "rm -rf dist && tsc -p .",
    "install:clean": "rm -rf node_modules && npm install",
    "install:nuke": "rm -rf node_modules && rm -f package-lock.json && npm install",
    "pm2:start": "pm2 start ecosystem.config.js --only vuebot && pm2:logs",
    "pm2:stop": "pm2 stop vuebot",
    "pm2:info": "pm2 info vuebot",
    "pm2:restart": "pm2 restart vuebot",
    "pm2:logs": "pm2 logs vuebot --lines=200"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Elfayer/vue-land-bot.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "official",
    "discord",
    "discord.js",
    "klasa",
    "typescript",
    "bot"
  ],
  "author": "Elfayer",
  "contributors": [
    "Elfayer",
    "sustained <sustained.dissonance@gmail.com> [https://sustained.name/]"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Elfayer/vue-land-bot/issues"
  },
  "homepage": "https://github.com/Elfayer/vue-land-bot#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "common-tags": "^1.8.0",
    "discord.js": "github:discordjs/discord.js#master",
    "dotenv": "^8.1.0",
    "esm": "^3.2.25",
    "fuse.js": "^3.4.6",
    "github-api": "^3.3.0",
    "hjson": "^3.1.2",
    "klasa": "github:dirigeants/klasa#settings",
    "module-alias": "^2.2.2",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@discordjs/collection": "^0.1.3",
    "@types/common-tags": "^1.8.0",
    "@types/node": "^13.1.7",
    "@types/node-fetch": "^2.5.4",
    "@types/ws": "^7.2.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2"
  },
  "engines": {
    "node": ">=12"
  },
  "_moduleAliases": {
    "@base": "dist",
    "@arguments": "dist/arguments",
    "@commands": "dist/commands",
    "@events": "dist/events",
    "@extendables": "dist/extendables",
    "@finalizers": "dist/finalizers",
    "@inhibitors": "dist/inhibitors",
    "@languages": "dist/languages",
    "@libraries": "dist/lib",
    "@monitors": "dist/monitors",
    "@providers": "dist/providers",
    "@schemas": "dist/schemas",
    "@serializers": "dist/serializers",
    "@constants": "dist/lib/constants",
    "@services": "dist/lib/services",
    "@structures": "dist/lib/structures",
    "@templates": "dist/lib/templates",
    "@utilities": "dist/lib/utilities"
  },
  "engines": {
    "node": ">=10"
  }
}
