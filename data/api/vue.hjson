          see: [
            {
              text: Guide - Computed Properties
              link: https://vuejs.org/v2/guide/computed.html
            }
          ]
        }
        {
          title: methods
          description: Methods to be mixed into the Vue instance.
          type: '{ [key: string]: Function'
          link: https://vuejs.org/v2/api/#methods
