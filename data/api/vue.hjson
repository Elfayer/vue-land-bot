          type: Array<string>
          default: '["{{", "}}"]'
          link: https://vuejs.org/v2/api/#delimiters
          usage: {
            lang: js
            code: '''
                new Vue({
                    delimiters: ['${', '}']
                })
            '''
          }
        }
        {
          title: functional
